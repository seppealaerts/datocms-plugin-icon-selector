{
  "name": "datocms-plugin-lucide-icon-selector",
  "description": "A plugin for DatoCMS that provides a searchable icon selector using Lucide Icons",
  "homepage": "https://github.com/seppealaerts/datocms-plugin-icon-selector",
  "private": false,
  "version": "0.1.0",
  "author": "Seppe Alaerts <hello@seppealaerts.be>",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "datocms-plugin",
    "lucide",
    "icon",
    "selector"
  ],
  "datoCmsPlugin": {
    "title": "DatoCMS plugin test",
    "coverImage": "docs/cover-1200x800.png",
    "previewImage": "docs/preview.mp4",
    "entryPoint": "dist/index.html",
    "permissions": []
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "datocms-plugin-sdk": "*",
    "datocms-react-ui": "*",
    "lucide-react": "^0.552.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.1"
  }
}
